<div class="row">
    <div class="col-lg-12">


            <div class="ibox">
                <div class="ibox-content">


                    <div class="project-list">

                        <table class="table table-hover">
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>课程名称</th>
                                <th>教师名称</th>
	                            <th style="text-align:center">操作</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%=console.log(datas)%>
                            <%var i = 0;%>
                            <% _.each(datas, function(item){ %>
                            <tr>
                                <td ><%=++i%></td>
                                <td class="project-title">
                                  <span><%=item.courceid.name%></span>
                                </td>
                                <td class="project-completion">
	                                <span>

		                                教师名字
	                                </span>
                                </td>

                                <td style="text-align:center" class="project-actions">
                                    <button  keyvalue="<%=item.id%>" class="btn btn-xs btn-info btn_course_in_sign" type="button"><i  class="fa fa-paste"></i> 取消报名</button>
                                </td>
                            </tr>
							<%})%>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

    </div>
</div>