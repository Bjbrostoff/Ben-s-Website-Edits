<div class="row">
    <div class="col-lg-12">


            <div class="ibox">
                <div class="ibox-content">


                    <div class="project-list">

                        <table class="table table-hover">
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>课程名称</th>
                                <th>教师名称</th>
                                <th>最近一次上课的时间</th>
	                            <th>上课地点</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%=console.log(datas)%>
                            <%var i = 0;%>
                            <% _.each(datas, function(item){ %>
                            <tr>
                                <td ><%=++i%></td>
                                <td class="project-title">
                                  <span><%=item.courceid.name%></span>
                                </td>
                                <td class="project-completion">
	                                <span>

		                                教师名字
	                                </span>
                                </td>
	                            <td>
		                            <span>
			                            <%=item.nextdate%>

		                            </span>

	                            </td>
	                            <td>
		                            <span>
			                            <%=item.nextaddress%>
		                            </span>

	                            </td>
                                <td class="project-actions">
                                    <button   keyvalue="<%=item._id%>" class="btn btn-xs btn-info" type="button"><i  class="fa fa-paste"></i> 查看</button>
                                </td>
                            </tr>
							<%})%>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

    </div>
</div>