<div>
    <% for(var i = 0;i < dateInfo.length;i++){ %>
    <div class="row">
        <div class="col-md-11 col-md-offset-1">
            <strong><%= dateInfo[i].day %></strong>
        </div>
        <div class="col-md-12">
            <% for(var j = 0;j < dateInfo[i].detail.length;j++){ %>
            <div class="row">
                <div class="col-md-11 col-md-offset-2">
                    课程名称：<%= dateInfo[i].detail[j].name %>
                </div>
            </div>
            <div class="row">
                <div class="col-md-2 col-md-offset-2">
                    参加学生：
                </div>
                <div class="col-md-5">
                    <% for(var g = 0;g < dateInfo[i].detail[j].students.length;g++){ %>
                    <div class="row">
                        <%= dateInfo[i].detail[j].students[g] %>
                    </div>
                    <% } %>
                </div>
            </div>

            <% } %>

        </div>
    </div>
    <% } %>
</div>
